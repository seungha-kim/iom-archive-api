{
  "name": "iom-archive-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "debug:watch": "nodemon -x npm run debug",
    "debug": "node --inspect ./node_modules/.bin/ts-node ./src/index.ts",
    "start": "ts-node src/index.ts",
    "lint": "prettier \"./src/**/*.{ts,tsx}\"",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "doc": "typedoc --out docs ./src",
    "test": "jest",
    "test:watch": "nodemon -x npm run test"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^2.0.0-rc.6",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "mysql2": "^1.5.3",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.7",
    "graphql-tools": "^3.0.4"
  },
  "devDependencies": {
    "@types/graphql": "^0.13.1",
    "@types/jest": "^23.1.3",
    "@types/node": "^8.0.29",
    "jest": "^23.2.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.7",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  }
}
